
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}




model User {
  id                     String                   @id @default(uuid())
  email                  String                   @unique
  first_name             String
  last_name              String
  phone_number           String
  password               String
  role                   UserRole
}


enum UserRole {
user
admin
}